{
  "cSpell.words": [
    "adminsortable",
    "Almarai",
    "asgi",
    "corsheaders",
    "djangorestframework",
    "faicon",
    "markdownx",
    "passwordreset",
    "pdfkit",
    "precompiler",
    "simplejwt",
    "summernote",
    "takwine",
    "wkhtmltopdf"
  ],
  "compile-hero.disable-compile-files-on-did-save-code": false,
  "python.analysis.typeCheckingMode": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "hide-files.files": [],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "black",
  "python.analysis.autoImportCompletions": true,
}